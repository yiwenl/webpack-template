{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run html:watch & NODE_ENV=development webpack-dev-server --open --mode development & node ./tasks/watch-asset.js & node ./tasks/shader-watcher.js",
    "start:windows": "concurrently \"set NODE_ENV=development&&webpack-dev-server --open --mode development\" \"npm run html:watch\" \"npm run shader\" \"npm run assets\"",
    "build:dev": "npm run html & NODE_ENV=development webpack --progress --profile --colors",
    "build": "NODE_ENV=production npm run html & NODE_ENV=production webpack --progress --profile --colors --mode production",
    "watch": "webpack --watch",
    "html": "node ./tasks/html.js",
    "assets": "node ./tasks/watch-asset.js",
    "shader": " node ./tasks/shader-watcher.js",
    "html:watch": "node ./tasks/html-watch.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
